{
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/index.py"
    }
  ]
}

{
  "build": {
    "env": {
      "OPENCV_VERSION": "4.5.4.58"
    },
    "env": {
      "CXX": "g++-8",
      "CC": "gcc-8"
    },
    "scripts": {
      "build": "python3 -m pip install -r requirements.txt"
    }
  }
}
